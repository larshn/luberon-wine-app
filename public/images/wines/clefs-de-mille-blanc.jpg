		    <script>
    (function () {
      // ---------- Einstellung ----------
      var COOKIE_NAME = 'lw_popup_seen';
      var COOKIE_DAYS = 365; // Wie lange "gesehen" gemerkt wird

      // ---------- Cookie-Helper ----------
      function setCookie(name, value, days) {
        var expires = "";
        if (days) {
          var d = new Date();
          d.setTime(d.getTime() + (days*24*60*60*1000));
          expires = "; expires=" + d.toUTCString();
        }
        document.cookie = name + "=" + encodeURIComponent(value) + expires + "; path=/";
      }
      function getCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) === ' ') c = c.substring(1, c.length);
          if (c.indexOf(nameEQ) === 0) return decodeURIComponent(c.substring(nameEQ.length, c.length));
        }
        return null;
      }

      // Bereits gesehen? -> Abbruch
      if (getCookie(COOKIE_NAME) === '1') return;

      // ---------- Popup ----------
      function initLagerumzugPopup() {
        // Doppelt-Init verhindern
        if (document.getElementById('lw-overlay')) return;

        // --- Styles einfgen ---
        var style = document.createElement('style');
        style.id = 'lw-popup-styles';
        style.textContent = "\
#lw-overlay { position: fixed; inset: 0; background: rgba(0,0,0,.45); display: flex; align-items: center; justify-content: center; z-index: 999999; padding: 20px; }\
#lw-modal { background: #fff; color: #000; max-width: 640px; width: 100%; border-radius: 10px; box-shadow: 0 10px 40px rgba(0,0,0,.2); padding: 22px 22px 18px; position: relative; line-height: 1.45; font-family: system-ui, -apple-system, Segoe UI, Roboto, Arial, sans-serif; }\
#lw-modal br { display: inline !important; } /* oder: block; */\
#lw-close { position: absolute; top: 10px; right: 10px; border: none; background: transparent; font-size: 22px; cursor: pointer; line-height: 1; padding: 6px; border-radius: 8px; color: #000!important }\
#lw-close:focus { outline: 2px solid #fff!important; outline-offset: 2px; }\
#lw-title { font-weight: 700; font-size: 18px; margin: 0 28px 8px 0; }\
#lw-desc { margin: 0; }";
        document.head.appendChild(style);

        // --- DOM erzeugen ---
        var overlay = document.createElement('div');
        overlay.id = 'lw-overlay';

        var modal = document.createElement('div');
        modal.id = 'lw-modal';
        modal.setAttribute('role', 'dialog');
        modal.setAttribute('aria-modal', 'true');
        modal.setAttribute('aria-labelledby', 'lw-title');
        modal.setAttribute('aria-describedby', 'lw-desc');

        var closeBtn = document.createElement('button');
        closeBtn.id = 'lw-close';
        closeBtn.setAttribute('aria-label', 'Schlieen');
        closeBtn.innerText = '';

        var title = document.createElement('h2');
        title.id = 'lw-title';
        title.textContent = 'LAGERUMZUG';

        var desc = document.createElement('p');
        desc.id = 'lw-desc';
        desc.innerHTML = 'Von <strong>Donnerstag, 18.09.25, 11&nbsp;Uhr</strong> bis <strong>Montag, 22.09.25, 9&nbsp;Uhr</strong> ist leider kein Versand mglich! Aber dann wird Ihr Paket sofort gepackt und verschickt!<p></p><p>Vielen Dank fr Ihr Verstndnis!</p>';

        modal.appendChild(closeBtn);
        modal.appendChild(title);
        modal.appendChild(desc);
        overlay.appendChild(modal);
        document.body.appendChild(overlay);

        // Cookie sofort setzen -> Popup erscheint pro Besucher nur 1x
        setCookie(COOKIE_NAME, '1', COOKIE_DAYS);

        // Scroll der Seite whrend Popup verhindern
        var prevOverflow = document.body.style.overflow;
        document.body.style.overflow = 'hidden';

        // --- Schlieen-Logik ---
        function close() {
          document.body.style.overflow = prevOverflow || '';
          overlay.remove();
          var s = document.getElementById('lw-popup-styles');
          if (s) s.remove();
        }

        closeBtn.addEventListener('click', close);
        overlay.addEventListener('click', function (e) {
          if (e.target === overlay) close();
        });
        document.addEventListener('keydown', function onEsc(e) {
          if (e.key === 'Escape') {
            document.removeEventListener('keydown', onEsc);
            close();
          }
        });

        // Fokus auf Close-Button zur Tastaturbedienung
        closeBtn.focus();
      }

      // DOM bereit? Dann starten (auch wenn Footer nach DOMContentLoaded geladen wird)
      if (document.readyState === 'loading') {
        //document.addEventListener('DOMContentLoaded', initLagerumzugPopup);
      } else {
        //initLagerumzugPopup();
      }
    })();
    </script>
